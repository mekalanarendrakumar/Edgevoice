option_settings:
  aws:elasticbeanstalk:environment:process:default:
    HealthCheckPath: /upload
    HealthCheckInterval: 30
    HealthyThreshold: 3
    UnhealthyThreshold: 5
    Port: 5000
    Protocol: HTTP
    StickinessEnabled: true
    StickinessLBCookieDuration: 86400
    StickinessType: lb_cookie
